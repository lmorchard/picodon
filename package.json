{
  "name": "picodon",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development npm-run-all --parallel server:dev watch",
    "server:dev": "webpack-dev-server --hot --watch --no-color",
    "server": "node index.js",
    "build": "webpack",
    "format": "prettier --write ./server/**/*.js ./client/src/**/*.js",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint --fix .",
    "watch": "npm-run-all --parallel watch:*",
    "watch:lint": "npm-run-all --parallel watch:lint:*",
    "watch:lint:js": "onchange --await-write-finish -d 1000 -p -v \"server/**/*.js\" \"client/src/**/*.js\" -- npm run lint:js"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/hello-express"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "express-session": "^1.15.6",
    "nedb": "^1.8.0",
    "nedb-promises": "^3.0.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.5",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-promise": "^0.6.0",
    "uuid": "^3.3.2",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "@babel/preset-react": "^7.0.0-rc.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta.6",
    "body-parser": "^1.18.3",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-mozilla": "^0.15.2",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "morgan": "^1.9.0",
    "node-fetch": "^2.2.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "onchange": "^4.1.0",
    "prettier": "^1.14.2",
    "style-loader": "^0.22.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
