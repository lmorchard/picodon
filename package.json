{
  "name": "hello-express",
  "version": "0.0.1",
  "description": "A simple Node app built on Express, instantly up and running.",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel server:dev",
    "server:dev": "nodemon --delay 2500 --ignore './client/*' --exec webpack-dev-server --hot --watch --no-color",
    "server": "node index.js",
    "build": "webpack",
    "format": "prettier --write ./server/**/*.js ./client/src/**/*.js"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "nedb": "^1.8.0",
    "nedb-promises": "^3.0.1",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.5",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-promise": "^0.6.0",
    "uuid": "^3.3.2",
    "ws": "^6.0.0"
  },
  "engines": {
    "node": "8.x"
  },
  "repository": {
    "url": "https://glitch.com/edit/#!/hello-express"
  },
  "license": "MIT",
  "keywords": [
    "node",
    "glitch",
    "express"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "@babel/preset-react": "^7.0.0-rc.3",
    "babel-loader": "^8.0.0-beta.6",
    "body-parser": "^1.18.3",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "morgan": "^1.9.0",
    "node-fetch": "^2.2.0",
    "nodemon": "^1.18.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.2",
    "style-loader": "^0.22.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
